    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.6",
    "web-vitals": "^5.1.0"
  }
