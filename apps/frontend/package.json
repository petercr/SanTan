    "@tanstack/router-plugin": "^1.134.9",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/css-utils": "^0.1.6",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "groq": "^4.12.0",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.552.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.9.5",
    "tailwindcss": "^4.1.16",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
